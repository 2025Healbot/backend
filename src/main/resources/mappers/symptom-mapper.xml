<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hospital.boot.domain.symptom.model.mapper.SymptomMapper">
    <select id="symptomDetails" resultType="String">
        SELECT SYMPTOM_DETAIL FROM SYMPTOM WHERE SYMPTOM_NAME = #{symptom}
    </select>
</mapper>